    "@types/react-dom": "18.0.11",
    "concurrently": "^8.0.1",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1"
  }
